# API密钥配置（复制为.env并填入真实密钥）

# DeepSeek API（生成模型）
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# OpenAI API（审计和评估模型）
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API（可选备用）
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# 本地模型配置（如果使用）
LOCAL_MODEL_PATH=./models/model_name
LOCAL_MODEL_TYPE=ollama  # ollama / vllm / transformers

# 日志配置
LOG_LEVEL=INFO
DEBUG=False
